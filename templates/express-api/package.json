{
  "name": "latitude55-task-list",
  "version": "1.6.0",
  "private": true,
  "description": "Latitude55 task list",
  "main": "src/index.js",
  "scripts": {
    "build": "sls package",
    "ci:deploy": "sls deploy --aws-s3-accelerate",
    "ci:remove": "sls remove",
    "ci:configure": "sls config credentials",
    "format": "prettier --check \"**/*.ts\"",
    "format:fix": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "start": "sls offline start",
    "pretest": "docker-compose up -d db-test",
    "test": "NODE_ENV=test jest --runInBand",
    "posttest": "docker-compose down -v",
    "test:watch": "NODE_ENV=test jest --runInBand --watch",
    "test:cov": "jest --runInBand --coverage",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "doctoc": "doctoc ./README.md"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hloughrey/latitude55-task-list.git"
  },
  "dependencies": {
    "@types/express": "4.17.21",
    "@vendia/serverless-express": "4.10.4",
    "body-parser": "1.20.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cors": "2.8.5",
    "express": "4.18.2",
    "express-validator": "6.15.0",
    "pg": "8.7.3",
    "pg-format": "1.0.4",
    "reflect-metadata": "0.1.13",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/git": "10.0.1",
    "@types/aws-lambda": "8.10.71",
    "@types/jest": "27.4.0",
    "@types/node": "14.14.25",
    "@types/pg-format": "1.0.2",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "doctoc": "2.1.0",
    "eslint": "8.54.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-unicorn": "41.0.0",
    "fork-ts-checker-webpack-plugin": "7.2.1",
    "husky": "7.0.0",
    "jest": "29.7.0",
    "json-schema-to-ts": "1.5.0",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1",
    "serverless": "3.2.1",
    "serverless-dotenv-plugin": "3.12.2",
    "serverless-offline": "8.5.0",
    "serverless-plugin-canary-deployments": "0.7.1",
    "serverless-webpack": "5.11.0",
    "supertest": "6.2.2",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.2",
    "webpack": "5.69.1"
  },
  "volta": {
    "node": "18.13.0",
    "npm": "8.19.3"
  }
}
